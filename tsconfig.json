{
	"compilerOptions": {
		"target": "ES2022",
		"lib": [
			"ES2022",
			"WebWorker"
		],
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"noEmit": true,
		"isolatedModules": true,
		"types": [
			"@cloudflare/workers-types"
		],
		"resolveJsonModule": true,
		"allowJs": true,
		"forceConsistentCasingInFileNames": true,
		"skipLibCheck": true
	},
	"include": [
		"src/**/*.ts"
	]
}
